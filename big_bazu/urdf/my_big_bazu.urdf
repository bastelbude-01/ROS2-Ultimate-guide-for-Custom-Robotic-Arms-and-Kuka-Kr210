<?xml version="1.0"?>
<robot name="my_big_bazu">

                <!-- Farben -->

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="yellow">
        <color rgba="1 1 0 1"/>
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="green">
        <color rgba="0.0 1.0 0.0 1.0"/>
    </material>

    <material name="red">
        <color rgba="1.0 0.0 0.0 1.0"/>
    </material>

    <!-- World Link-->

  <link name="world"/>
  
    <!-- Base Link-->

    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.13" length="0.01"/>
            </geometry>
            <material name="black"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>            
        </visual>

    <collision>
        <geometry>
            <cylinder radius="0.13" length="0.01"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>

    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.0"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    </link>

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>    
        <!-- World Base Joint-->
    <joint name="world_base_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/> 
        <origin xyz="0.0 0.0 0.005" rpy="0.0 0.0 0.0"/>
        <dynamics damping="10" friction="1.0"/>             
    </joint>

    <!-- Base Aufbau-->

    <link name="body_link">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.053"/>
            </geometry>
            <material name="black"/>
            <origin rpy="0 0 0" xyz="0 0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.053"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0.0"/>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    <gazebo reference="body_link">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Body Base Joint-->

    <joint name="body_base_joint" type="fixed">
        <parent link="base_link"/>
        <child link="body_link"/>
        <origin xyz="0.0 0.0 0.031" rpy="0.0 0.0 0.0"/>           
    </joint>
    
    <!-- Rotations Teller-->

    <link name="teller_link">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.005"/>
            </geometry>
            <material name="orange"/>
            <origin rpy="0 0 0" xyz="0 0 0.0025"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.005"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0.0025"/>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.0025"/>
            <mass value="0.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    <gazebo reference="teller_link">
        <material>Gazebo/Orange</material>
    </gazebo>

                <!-- Joint NR 1-->
    <joint name="teller_joint_1" type="revolute">
        <origin xyz="0.0 0.0 0.029" rpy="0.0 0.0 0.0"/>
        <parent link="body_link"/>
        <child link="teller_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-1.57" upper="1.57" velocity="50.0" effort="1000.0"/>
    </joint>

    <!-- Teller Aufbau-->

    <link name="ta_link">
        <visual>
            <geometry>
                <box size="0.06 0.02 0.06"/>
            </geometry>
            <material name="orange"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.06 0.02 0.06"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    <gazebo reference="ta_link">
        <material>Gazebo/Orange</material>
    </gazebo>

    <joint name="ta_joint" type="fixed">
        <origin xyz="0.015 0.015 0.032" rpy="0.0 0.0 0.0"/>
        <parent link="teller_link"/>
        <child link="ta_link"/>
    </joint>

    <!-- Gelenk Scheibe-->

    <link name="plate_1_link">
        <visual>
            <geometry>
                <cylinder radius="0.0175" length="0.003"/>
            </geometry>
            <material name="orange"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.0175" length="0.003"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    <gazebo reference="plate_1_link">
        <material>Gazebo/Orange</material>
    </gazebo>

    <joint name="ta_plate_joint" type="fixed">
        <origin xyz="0.01 -0.0115 0.01" rpy="1.57 0.0 0.0"/>
        <parent link="ta_link"/>
        <child link="plate_1_link"/>
    </joint>

    <!-- ARM 1 /// Arm Scheibe 1-->

    <link name="plate_2_link">
        <visual>
            <geometry>
                <cylinder radius="0.0175" length="0.023"/>
            </geometry>
            <material name="white"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.0175" length="0.023"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    <gazebo reference="plate_2_link">
        <material>Gazebo/White</material>
    </gazebo>
                <!-- Joint NR 2-->
    <joint name="arm_lift_joint_2" type="revolute">
        <parent link="plate_1_link"/>
        <child link="plate_2_link"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-2.8" upper="0.35" velocity="50.0" effort="1000.0"/>
        <origin xyz="0 0 0.013" rpy="0 0 1.17"/>
    </joint>
        
    <!-- Arm 1-->

    <link name="arm_1_link">
        <visual>
            <geometry>
                <box size="0.035 0.13 0.017" />
            </geometry>
            <material name="white"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.035 0.13 0.017" />
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
        <inertial>
